{
  "private": true,
  "description": "Adaptive Framework UI Apps and Libraries",
  "engines": {
    "npm": ">=8.0.0",
    "node": ">=16.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/afw-org/afw.git"
  },
  "scripts": {
    "clean": "npm run clean:client && npm run clean:test && npm run clean:react && npm run clean:mui && npm run clean:monaco && npm run clean:bindings",
    "clean:client": "rimraf src/afw_client/javascript/build",
    "clean:test": "rimraf src/afw_test/javascript/build",
    "clean:react": "rimraf src/afw_components/react/core/build",
    "clean:mui": "rimraf src/afw_components/react/material/build",
    "clean:monaco": "rimraf src/afw_components/react/monaco/build",
    "clean:bindings": "rimraf src/afw/generated/javascript_bindings/build && rimraf src/afw_lmdb/generated/javascript_bindings/build",
    "build": "npm run build:modules && npm run build:apps",
    "build-p": "npm run build-p:modules && npm run build:apps",
    "build:modules": "npm run build:bindings && npm run build:client && npm run build:react && npm run build:mui && npm run build:monaco && npm run build:test",
    "build-p:modules": "npm run build-p:bindings && npm run build-p:client && npm run build-p:react && npm-run-all --parallel --aggregate-output build-p:test build-p:mui build-p:monaco",
    "build:apps": "npm run build:admin",
    "build:admin": "npm run --prefix src/afw_app/admin build",
    "build:client": "npm run --prefix src/afw_client/javascript build",
    "build-p:client": "npm run --prefix src/afw_client/javascript build-p",
    "build:bindings": "npm run build:bindings:afw && npm run build:bindings:afw_lmdb",
    "build-p:bindings": "npm-run-all --aggregate-output --parallel build-p:bindings:afw build-p:bindings:afw_lmdb",
    "build:bindings:afw": "npm run --prefix src/afw/generated/javascript_bindings build",
    "build-p:bindings:afw": "npm run --prefix src/afw/generated/javascript_bindings build-p",
    "build:bindings:afw_ldap": "npm run --prefix src/afw_ldap/generated/javascript_bindings build",
    "build-p:bindings:afw_ldap": "npm run --prefix src/afw_ldap/generated/javascript_bindings build-p",
    "build:bindings:afw_lmdb": "npm run --prefix src/afw_lmdb/generated/javascript_bindings build",
    "build-p:bindings:afw_lmdb": "npm run --prefix src/afw_lmdb/generated/javascript_bindings build-p",
    "build:react": "npm run --prefix src/afw_components/react/core build",
    "build-p:react": "npm run --prefix src/afw_components/react/core build-p",
    "build:mui": "npm run --prefix src/afw_components/react/material build",
    "build-p:mui": "npm run --prefix src/afw_components/react/material build-p",
    "build:monaco": "npm run --prefix src/afw_components/react/monaco build",
    "build-p:monaco": "npm run --prefix src/afw_components/react/monaco build-p",
    "build:test": "npm run --prefix src/afw_test/javascript build",
    "build-p:test": "npm run --prefix src/afw_test/javascript build-p",
    "eslint": "eslint src/afw_client/javascript/src src/afw_components/react/core/src src/afw_components/react/material/src src/afw_app/admin/src",
    "test": "npm run test:client && npm run test:mui && npm run test:admin",
    "test-p": "npm-run-all --parallel --aggregate-output test:client test:mui test:admin-p",
    "test:client": "npm run --prefix src/afw_client/javascript test",
    "test:react": "npm run --prefix src/afw_components/react/core test",
    "test:mui": "npm run --prefix src/afw_components/react/material test",
    "test:admin": "npm run --prefix src/afw_app/admin test",
    "test:admin-p": "npm run --prefix src/afw_app/admin test-p",
    "storybook": "start-storybook -c src/afw_components/.storybook -s public --no-dll",
    "storybook:build": "build-storybook -c src/afw_components/.storybook -o ./doc/storybook"
  },
  "workspaces": [
    "src/afw/generated/javascript_bindings",
    "src/afw_client/javascript",
    "src/afw_components/react/core",
    "src/afw_components/react/material",
    "src/afw_components/react/monaco",
    "src/afw_test/javascript"
  ],
  "dependencies": {
    "@axe-core/react": "^4.1.1",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/plugin-transform-runtime": "^7.13.9",
    "@babel/preset-react": "^7.13.13",
    "@babel/preset-typescript": "^7.13.0",
    "@date-io/moment": "^2.11.0",
    "@emotion/css": "^11.9.0",
    "@emotion/react": "^11.4.1",
    "@emotion/styled": "^11.3.0",
    "@mui/icons-material": "^5.0.0",
    "@mui/lab": "^5.0.0-alpha.47",
    "@mui/material": "^5.0.0",
    "@types/react-dom": "^17.0.2",
    "cpx": "^1.5.0",
    "cross-env": "^7.0.3",
    "eslint-plugin-jest": "^26.0.0",
    "esm": "^3.2.25",
    "jspreadsheet-ce": "^4.7.1",
    "material-design-icons-iconfont": "^6.1.0",
    "mdi-material-ui": "^7.1.0",
    "moment-timezone": "^0.5.33",
    "monaco-editor": "^0.33.0",
    "monaco-languages": "^2.11.1",
    "npm-run-all": "^4.1.5",
    "prop-types": "^15.7.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "^5.0.0",
    "rimraf": "^3.0.0",
    "typeface-roboto": "^1.1.13",
    "typeface-roboto-mono": "^1.1.13",
    "typescript": "^4.3.2",
    "util": "^0.12.4",
    "uuid": "^8.3.2",
    "vis-data": "^7.1.2",
    "vis-network": "^9.0.4",
    "vis-util": "^5.0.2",
    "web-streams-polyfill": "^3.0.3"
  },
  "devDependencies": {
    "@swc/cli": "^0.1.51",
    "@swc/core": "^1.2.139",
    "@testing-library/cypress": "^8.0.1",
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/react": "^12.1.2",
    "@testing-library/react-hooks": "^7.0.2",
    "@testing-library/user-event": "^13.0.1",
    "msw": "^0.39.1",
    "react-is": "^17.0.1",
    "source-map-explorer": "^2.5.2",
    "webpack-bundle-analyzer": "^4.3.0"
  },
  "jest": {
    "resetMocks": false
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
